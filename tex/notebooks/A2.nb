Cell[TextData[{
 "Compute our polynomial basis ",
 StyleBox["bp",
  FontWeight->"Bold"]
}], "Subsubsection",
 CellChangeTimes->{{3.727533051340085*^9, 3.727533071216785*^9}, {
  3.727538436386805*^9, 3.727538437305244*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"bp", "[", 
    RowBox[{"n_", ",", " ", "x_", ",", " ", "y_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l", ",", "m", ",", "\[Mu]", ",", "\[Nu]"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"l", "=", 
       RowBox[{"Floor", "[", 
        SqrtBox["n"], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"m", "=", 
       RowBox[{"n", "-", 
        SuperscriptBox["l", "2"], "-", "l"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Mu]", "=", 
       RowBox[{"l", "-", "m"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Nu]", "=", 
       RowBox[{"l", "+", "m"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"EvenQ", "[", "\[Nu]", "]"}], ",", 
        RowBox[{
         SuperscriptBox["x", 
          FractionBox["\[Mu]", "2"]], 
         SuperscriptBox["y", 
          FractionBox["\[Nu]", "2"]]}], ",", 
        RowBox[{
         SuperscriptBox["x", 
          FractionBox[
           RowBox[{"\[Mu]", "-", "1"}], "2"]], 
         SuperscriptBox["y", 
          FractionBox[
           RowBox[{"\[Nu]", "-", "1"}], "2"]], 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"], "-", 
           SuperscriptBox["y", "2"]}]]}]}], "]"}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.7275327970440807`*^9, 3.7275329250224113`*^9}, {
  3.727532981271962*^9, 3.7275330241172237`*^9}, {3.7275335806280203`*^9, 
  3.727533594697996*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "Compute our greens basis ",
 StyleBox["bg",
  FontWeight->"Bold"]
}], "Subsubsection",
 CellChangeTimes->{{3.727533051340085*^9, 3.727533071216785*^9}, {
  3.727538436386805*^9, 3.727538437305244*^9}, {3.727545264468409*^9, 
  3.7275452670267143`*^9}, {3.727555550128257*^9, 3.727555550302718*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"bg", "[", 
    RowBox[{"n_", ",", " ", "x_", ",", " ", "y_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l", ",", "m", ",", "\[Mu]", ",", "\[Nu]"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"l", "=", 
       RowBox[{"Floor", "[", 
        SqrtBox["n"], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"m", "=", 
       RowBox[{"n", "-", 
        SuperscriptBox["l", "2"], "-", "l"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Mu]", "=", 
       RowBox[{"l", "-", "m"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Nu]", "=", 
       RowBox[{"l", "+", "m"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Which", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"EvenQ", "[", "\[Nu]", "]"}], ",", 
        RowBox[{
         FractionBox[
          RowBox[{"\[Mu]", "+", "2"}], "2"], 
         SuperscriptBox["x", 
          FractionBox["\[Mu]", "2"]], 
         SuperscriptBox["y", 
          FractionBox["\[Nu]", "2"]]}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"\[Nu]", "\[Equal]", "1"}], " ", "&&", " ", 
         RowBox[{"\[Mu]", "\[Equal]", "1"}]}], ",", 
        SqrtBox[
         RowBox[{"1", "-", 
          SuperscriptBox["x", "2"], "-", 
          SuperscriptBox["y", "2"]}]], ",", "\[IndentingNewLine]", 
        RowBox[{"\[Mu]", ">", "1"}], ",", 
        RowBox[{
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"], "-", 
           SuperscriptBox["y", "2"]}]], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            FractionBox[
             RowBox[{"\[Mu]", "-", "3"}], "2"], 
            SuperscriptBox["x", 
             FractionBox[
              RowBox[{"\[Mu]", "-", "5"}], "2"]], 
            SuperscriptBox["y", 
             FractionBox[
              RowBox[{"\[Nu]", "-", "1"}], "2"]]}], "-", 
           RowBox[{
            FractionBox[
             RowBox[{"\[Mu]", "-", "3"}], "2"], 
            SuperscriptBox["x", 
             FractionBox[
              RowBox[{"\[Mu]", "-", "5"}], "2"]], 
            SuperscriptBox["y", 
             FractionBox[
              RowBox[{"\[Nu]", "+", "3"}], "2"]]}], "-", 
           RowBox[{
            FractionBox[
             RowBox[{"\[Mu]", "+", "3"}], "2"], 
            SuperscriptBox["x", 
             FractionBox[
              RowBox[{"\[Mu]", "-", "1"}], "2"]], 
            SuperscriptBox["y", 
             FractionBox[
              RowBox[{"\[Nu]", "-", "1"}], "2"]]}]}], ")"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"OddQ", "[", "l", "]"}], ",", 
        RowBox[{
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"], "-", 
           SuperscriptBox["y", "2"]}]], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox["x", 
             RowBox[{"l", "-", "3"}]]}], "+", 
           SuperscriptBox["x", 
            RowBox[{"l", "-", "1"}]], "+", 
           RowBox[{"4", 
            SuperscriptBox["x", 
             RowBox[{"l", "-", "3"}]], 
            SuperscriptBox["y", "2"]}]}], ")"}]}], ",", "\[IndentingNewLine]",
         "True", ",", 
        RowBox[{"3", 
         SuperscriptBox["x", 
          RowBox[{"l", "-", "2"}]], "y", " ", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"], "-", 
           SuperscriptBox["y", "2"]}]]}]}], "\[IndentingNewLine]", "]"}]}]}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7275327970440807`*^9, 3.7275329250224113`*^9}, {
   3.727532981271962*^9, 3.7275330241172237`*^9}, {3.7275335806280203`*^9, 
   3.727533594697996*^9}, 3.7275452686758347`*^9, {3.7275525513571157`*^9, 
   3.7275527956289473`*^9}, {3.727552859384865*^9, 3.727552886430558*^9}}]
