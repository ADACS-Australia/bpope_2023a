Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"p", "[", 
    RowBox[{"n_", ",", "lmax_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "g", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"g", "=", 
       RowBox[{"bg", "[", 
        RowBox[{"n", ",", "x", ",", "y"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Join", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"Evaluate", "[", 
          RowBox[{"g", "/.", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              SqrtBox[
               RowBox[{"1", "-", 
                SuperscriptBox["x", "2"], "-", 
                SuperscriptBox["y", "2"]}]], "\[Rule]", "0"}], ",", 
             RowBox[{"x", "\[Rule]", "0"}], ",", 
             RowBox[{"y", "\[Rule]", "0"}]}], "}"}]}], "]"}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Coefficient", "[", 
            RowBox[{"g", ",", 
             RowBox[{"bp", "[", 
              RowBox[{"j", ",", "x", ",", "y"}], "]"}]}], "]"}], "/.", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              SqrtBox[
               RowBox[{"1", "-", 
                SuperscriptBox["x", "2"], "-", 
                SuperscriptBox["y", "2"]}]], "\[Rule]", "0"}], ",", 
             RowBox[{"x", "\[Rule]", "0"}], ",", 
             RowBox[{"y", "\[Rule]", "0"}]}], "}"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", 
            RowBox[{
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"lmax", "+", "1"}], ")"}], "2"], "-", "1"}]}], "}"}]}],
          "]"}]}], "]"}]}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.727533095972856*^9, 3.727533184290205*^9}, {
