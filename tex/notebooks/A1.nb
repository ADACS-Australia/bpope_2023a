Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"bp", "[", 
    RowBox[{"n_", ",", " ", "x_", ",", " ", "y_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l", ",", "m", ",", "\[Mu]", ",", "\[Nu]"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"l", "=", 
       RowBox[{"Floor", "[", 
        SqrtBox["n"], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"m", "=", 
       RowBox[{"n", "-", 
        SuperscriptBox["l", "2"], "-", "l"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Mu]", "=", 
       RowBox[{"l", "-", "m"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Nu]", "=", 
       RowBox[{"l", "+", "m"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"EvenQ", "[", "\[Nu]", "]"}], ",", 
        RowBox[{
         SuperscriptBox["x", 
          FractionBox["\[Mu]", "2"]], 
         SuperscriptBox["y", 
          FractionBox["\[Nu]", "2"]]}], ",", 
        RowBox[{
         SuperscriptBox["x", 
          FractionBox[
           RowBox[{"\[Mu]", "-", "1"}], "2"]], 
         SuperscriptBox["y", 
          FractionBox[
           RowBox[{"\[Nu]", "-", "1"}], "2"]], 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"], "-", 
           SuperscriptBox["y", "2"]}]]}]}], "]"}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.7275327970440807`*^9, 3.7275329250224113`*^9}, {
  3.727532981271962*^9, 3.7275330241172237`*^9}, {3.7275335806280203`*^9, 
  3.727533594697996*^9}}]
}, Open  ]],

